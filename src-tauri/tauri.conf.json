{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "externalBin": [
      "binaries/ffprobe"
    ],
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Playa",
  "mainBinaryName": "Playa",
  "version": "0.3.9",
  "identifier": "com.cliambrown.playa",
  "plugins": {},
  "app": {
    "security": {
      "assetProtocol": {
        "scope": {
          "allow": [
            "**"
          ],
          "deny": [],
          "requireLiteralLeadingDot": false
        },
        "enable": true
      },
      "csp": "default-src 'self'; img-src  https://artworks.thetvdb.com/ 'self' asset: https://asset.localhost; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "Playa",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ]
  }
}